{% extends core['extended_layout'] ? core['extended_layout'] : 'CoreBundle:Base:base.layout.html.twig' %}


{% block container_content %}
    <div class="container">
        <div class="col-sm-2"></div>  
        <div class="col-md-8 container-elearning" style="margin-top: 80px"> 
            <div class="row">
                <h2 class="center">{{ 'elearning.title' | trans }}</h2>
                    {% for test in tests%}
                        <li><a href="{{ path('elearning_front_test', {test: test.id})}}">{{test.name}}</a></li>
                    {% endfor %}
            </div>
        </div>
        <div class="col-sm-2"></div>  
    </div>
{% endblock %}

